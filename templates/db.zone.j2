$TTL 604800
@         IN       SOA      ns1.{{ name_domain }}. admin.{{ name_domain }}. (
          2013120400 ; Serial
          604800     ; Refresh
          86400      ; Retry
          2419200    ; Expire
          604800 )   ; Negative Cache TTL
;
                   NS       ns1.{{ name_domain }}.
                   NS       ns2.{{ name_domain }}.
                   MX       10    mail
ns1.{{ name_domain }}.   A        172.17.0.3
ns2.{{ name_domain }}.   A        172.17.0.2 
{% for key in registrys %}
{{ registrys[key].name }}		{{ registrys[key].type }}		{{ registrys[key].ip }}
{% endfor %}

www            CNAME        ns1.{{ name_domain }}.

